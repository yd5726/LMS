<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TTActivity">
    <include layout="@layout/top_nav"/>
    <LinearLayout
        android:layout_margin="10dp"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <androidx.cardview.widget.CardView
            android:backgroundTint="@color/light_blue"
            app:cardCornerRadius="20dp"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <LinearLayout
                android:layout_margin="10dp"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
            <TableLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="교시"
                        style="@style/TEXT_STYLE_1"
                        android:layout_width="0dp"
                        android:layout_height="50dp"/>
                    <TextView
                        android:layout_weight="1"
                        android:gravity="center"
                        style="@style/TEXT_STYLE_1"
                        android:text="월"
                        android:layout_width="0dp"
                        android:layout_height="50dp"/>
                    <TextView
                        android:layout_weight="1"
                        android:gravity="center"
                        style="@style/TEXT_STYLE_1"
                        android:text="화"
                        android:layout_width="0dp"
                        android:layout_height="50dp"/>
                    <TextView
                        android:layout_weight="1"
                        android:gravity="center"
                        style="@style/TEXT_STYLE_1"
                        android:text="수"
                        android:layout_width="0dp"
                        android:layout_height="50dp"/>
                    <TextView
                        android:layout_weight="1"
                        android:gravity="center"
                        style="@style/TEXT_STYLE_1"
                        android:text="목"
                        android:layout_width="0dp"
                        android:layout_height="50dp"/>
                    <TextView
                        android:layout_weight="1"
                        android:gravity="center"
                        style="@style/TEXT_STYLE_1"
                        android:text="금"
                        android:layout_width="0dp"
                        android:layout_height="50dp"/>
                </TableRow>
            </TableLayout>
                <LinearLayout
                    android:layout_marginBottom="5dp"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:id="@+id/ln_layout0"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:background="#BABABA"
                        android:layout_height="match_parent"
                        android:orientation="vertical" />
                    <LinearLayout
                        android:id="@+id/ln_layout1"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:background="#66B1AB"
                        android:layout_height="match_parent"
                        android:orientation="vertical" />
                    <LinearLayout
                        android:background="#BABABA"
                        android:id="@+id/ln_layout2"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical" />
                    <LinearLayout
                        android:id="@+id/ln_layout3"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:background="#66B1AB"
                        android:layout_height="match_parent"
                        android:orientation="vertical" />
                    <LinearLayout
                        android:background="#BABABA"
                        android:id="@+id/ln_layout4"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical" />
                    <LinearLayout
                        android:id="@+id/ln_layout5"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:background="#66B1AB"
                        android:layout_height="match_parent"
                        android:orientation="vertical" />
                </LinearLayout>
                <LinearLayout
                    android:layout_marginBottom="5dp"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:id="@+id/ln_layout6"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:background="#BABABA"
                        android:layout_height="match_parent"
                        android:orientation="vertical" />
                    <LinearLayout
                        android:id="@+id/ln_layout7"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:background="#66B1AB"
                        android:layout_height="match_parent"
                        android:orientation="vertical" />
                    <LinearLayout
                        android:background="#BABABA"
                        android:id="@+id/ln_layout8"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical" />
                    <LinearLayout
                        android:id="@+id/ln_layout9"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:background="#66B1AB"
                        android:layout_height="match_parent"
                        android:orientation="vertical" />
                    <LinearLayout
                        android:background="#BABABA"
                        android:id="@+id/ln_layout10"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical" />
                    <LinearLayout
                        android:id="@+id/ln_layout11"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:background="#66B1AB"
                        android:layout_height="match_parent"
                        android:orientation="vertical" />
                </LinearLayout>
                <LinearLayout
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:id="@+id/ln_layout12"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:background="#BABABA"
                        android:layout_height="match_parent"
                        android:orientation="vertical" />
                    <LinearLayout
                        android:id="@+id/ln_layout13"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:background="#66B1AB"
                        android:layout_height="match_parent"
                        android:orientation="vertical" />
                    <LinearLayout
                        android:background="#BABABA"
                        android:id="@+id/ln_layout14"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical" />
                    <LinearLayout
                        android:id="@+id/ln_layout15"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:background="#66B1AB"
                        android:layout_height="match_parent"
                        android:orientation="vertical" />
                    <LinearLayout
                        android:background="#BABABA"
                        android:id="@+id/ln_layout16"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical" />
                    <LinearLayout
                        android:id="@+id/ln_layout17"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:background="#66B1AB"
                        android:layout_height="match_parent"
                        android:orientation="vertical" />
                </LinearLayout>
            </LinearLayout>
        </androidx.cardview.widget.CardView>
    </LinearLayout>
</LinearLayout>